import scrapy

class TutorialItem(scrapy.Item):
    # define the fields for your item here like:
    date_parse = scrapy.Field()
    link = scrapy.Field()
    status = scrapy.Field()
    title = scrapy.Field()
    date_news = scrapy.Field()
    description = scrapy.Field()
    link_img = scrapy.Field()

    # pass
